<?xml version="1.0" encoding="utf-8" ?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >
<mapper namespace="com.fairing.dao.UserDao">
    <insert id="addUser">
        insert into t_user (avatar,email,nickname,password,type,username)
        values (#{avatar},#{email},#{nickname},#{password},0,#{username})
    </insert>
</mapper>


